{
  "name": "draft-wysiwyg",
  "version": "0.0.15",
  "repository": "https://github.com/bkniffler/draft-wysiwyg",
  "main": "./lib",
  "keywords": [
    "draftjs",
    "draft-js",
    "editor",
    "wysiwyg",
    "drag",
    "drop",
    "react",
    "richtext"
  ],
  "scripts": {
    "start": "node ./example",
    "build": "babel --presets es2015,stage-0,react src/ --out-dir lib/",
    "heroku": "git push heroku master",
    "start:production": "NODE_ENV=production node ./example",
    "build:example": "webpack --config example/webpack.js",
    "patch": "npm run build; npm version patch --force; npm publish"
  },
  "dependencies": {
    "draft-js": "^0.1.0",
    "immutable": "^3.7.4",
    "superagent": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^1.0.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.2.0",
    "express": "^4.13.1",
    "multer": "^1.1.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.7",
    "react-portal": "^2.0.0",
    "react-portal-tooltip": "^1.0.4",
    "webpack": "^1.10.5",
    "webpack-dev-middleware": "^1.5.1"
  }
}
