{
  "name": "draft-wysiwyg",
  "version": "0.2.0-beta1",
  "author": "Benjamin Kniffler",
  "license": "MIT",
  "repository": "https://github.com/bkniffler/draft-wysiwyg",
  "main": "./lib",
  "engines": {
    "node": "5.x.x",
    "npm": "3.x.x"
  },
  "keywords": [
    "draftjs",
    "draft-js",
    "editor",
    "wysiwyg",
    "drag",
    "drop",
    "react",
    "richtext"
  ],
  "scripts": {
    "start": "node ./example",
    "build": "babel --presets es2015,stage-0,react src/ --out-dir lib/",
    "heroku": "git push heroku master",
    "start:production": "NODE_ENV=production node ./example",
    "build:example": "gulp build",
    "patch": "npm run build; npm version patch --force; npm publish"
  },
  "dependencies": {
    "draft-js": "^0.6.0",
    "draft-js-alignment-plugin": "latest",
    "draft-js-cleanup-empty-plugin": "latest",
    "draft-js-dnd-plugin": "^1.0.3",
    "draft-js-entity-props-plugin": "latest",
    "draft-js-focus-plugin": "^1.0.5",
    "draft-js-image-plugin": "latest",
    "draft-js-plugins-editor-wysiwyg": "latest",
    "draft-js-resizeable-plugin": "^1.0.4",
    "draft-js-table-plugin": "latest",
    "draft-js-toolbar-plugin": "latest",
    "immutable": "^3.7.4",
    "react-portal": "^2.1.0"
  },
  "devDependencies": {
    "express": "^4.13.1",
    "gulp": "^3.9.0",
    "multer": "^1.1.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-portal": "^2.0.0",
    "superagent": "^1.8.0",
    "wrappack": "^0.3.10"
  }
